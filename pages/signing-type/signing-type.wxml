      <block>
        <open-data type="userAvatarUrl"></open-data>
      </block>
    </view>
    <view class="user-name">{{userInfo.ownerNam}}</view>
  </view>
  <navigator url="/pages/card-list/card-list">
    <view class="item">
      <van-icon size="16" color="#61A5FF" name="card" />
