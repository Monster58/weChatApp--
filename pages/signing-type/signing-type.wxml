    </view>
  </navigator>
  <navigator url="/pages/payment-category/payment-category">
    <view class="item">
      <van-icon size="16" color="#61A5FF" name="gold-coin" />
      <view class="item-name">生活缴费</view>
    </view>
  </navigator>
</view>